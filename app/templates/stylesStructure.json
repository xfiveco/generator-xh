{
  "default": [
    { "name": "main.{{type}}" },
    {
      "dirname": "common",
      "children": [
        { "name": "_base.{{type}}" },
        { "name": "_utilities.{{type}}" },
        { "name": "_layout.{{type}}" }
      ]
    }, {
      "dirname": "setup",
      "children": [
        { "name": "_variables.{{type}}" },
        { "name": "_mixins.{{type}}" }
      ]
    }, {
      "dirname": "components"
    }, {
      "dirname": "vendor"
    }
  ],

  "sprites": [
    {
      "dirname": "setup",
      "children": [
        { "name": "_sprites.{{type}}" },
        {
          "name": "_sprites.{{type}}.mustache",
          "raw": true
        }
      ]
    }
  ],

  "wp": [
    {
      "dirname": "common",
      "children": [
        { "name": "_wordpress.{{type}}" }
      ]
    }
  ],

  "bootstrap": [
    {
      "dirname": "vendor",
      "children": [
        {
          "dirname": "bootstrap",
          "children": [
            { "name": "_variables.{{type}}" }
          ]
        }
      ]
    }
  ],

  "browserify": [
    {
      "dirname": "modules"
    }
  ]
}
